#page
  background-color: $backgroundDarkColor
  width: 100vw
  height: calc(100vh - 60px)
  float: left
  padding: 0
  overflow: auto
  position: relative

  & > *
    margin-bottom: $elementVSpacing
    display: block

  iframe
    width: 100%
    height: 100%
    border: 0
    padding: 0
    margin: 0

